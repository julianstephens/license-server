            "post": {
                "description": "creates a new API key",
                "tags": [
                    "auth"
                ],
                "summary": "Creates a token",
                "parameters": [
                    {
                        "description": "returning user info",
                        "name": "data",
                        "in": "body",
                        "required": true,
                        "schema": {
